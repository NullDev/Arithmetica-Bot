{
  "name": "arithmetica",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "description": "Discord Bot for counting. Allows arithmetic expressions.",
  "main": "src/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start:dev": "npm run lint && cross-env NODE_ENV=development node src/app.js",
